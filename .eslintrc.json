{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-new": 0,
    "max-classes-per-file": 0,
    "no-underscore-dangle": 0,
    "import/no-named-as-default": 0,
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}